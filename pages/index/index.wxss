page{ background:#f5f5f5;}
.indexbox{padding: 30rpx; box-sizing: border-box;}
.contbox{ margin-bottom: 30rpx; position: relative;}
.msgLis{ padding: 40rpx 30rpx; border-top: 2rpx solid #e1e1e1; line-height: 28rpx; }
